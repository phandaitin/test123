<form role="form" class="form-inline zt-form" id="zt-form" action="" method="POST" > 
               
               <div class="table-responsive">                  
                  <table class="table table-striped table-bordered table-hover">                                            
                     <thead>
                        <tr>
                           <th>#</th>                           
                           <th>Name</th>
                           <th>Email</th>
                           <th>Pass</th>
                          
                           <th>Action</th>
                        </tr>
                     </thead>
                     <tbody>
                        
                        <% for(let i=0 ; i < data.length ; i++ ) { %>
                           <tr>
                              <td><%= i+1 %></td>
                              <td><%= data[i].name %></td>
                              <td><%= data[i].email %></td>
                              <td><%= data[i].pass %></td> 
                              
                              <td>                                 
                                 <a href="#"   style=" font-size:15px;" class="btn btn-edit glyphicon glyphicon-edit"> </a>
                                 <a href="#"  style=" font-size:15px;" class="btn btn-delete glyphicon glyphicon-trash"> </a>
                              </td>
                           </tr>
                        <% } %>

                     </tbody>
                  </table>
               </div>
            </form> 